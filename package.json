{
  "name": "jthooks",
  "description": "create a github webhook from the command-line",
  "version": "0.1.1",
  "author": "C J Silverio <ceejceej@gmail.com>",
  "bin": {
    "jthooks": "hook-cli.js"
  },
  "bugs": {
    "url": "https://github.com/ceejbot/jthooks/issues"
  },
  "config": {
    "blanket": {
      "pattern": [
        "hook-cli.js",
        "index.js"
      ],
      "data-cover-never": [
        "node_modules",
        "test"
      ],
      "output-reporter": "spec"
    }
  },
  "dependencies": {
    "chalk": "~1.1.1",
    "github": "~0.2.3",
    "lodash": "~3.10.1",
    "yargs": "~3.31.0"
  },
  "devDependencies": {
    "mocha": "~2.3.3",
    "mocoverage": "~1.0.0",
    "must": "~0.13.1",
    "patched-blanket": "~1.0.1",
    "sinon": "~1.17.2"
  },
  "homepage": "https://github.com/ceejbot/jthooks",
  "keywords": [
    "github",
    "webhooks",
    "cli"
  ],
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ceejbot/jthooks.git"
  },
  "scripts": {
    "test": "mocha -t 5000 --check-leaks --ui exports --require patched-blanket -R mocoverage test/test-*.js",
    "test-travis": "mocha -R spec -t 5000 test/test-*.js"
  }
}
